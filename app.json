{
  "name": "erxes-logger",
  "description": "Logger module for erxes",
  "keywords": ["node", "express", "graphql", "apollo"],
  "website": "https://erxes.io",
  "repository": "https://github.com/erxes/erxes-logger",
  "logo": "https://raw.githubusercontent.com/erxes/erxes/master/public/images/logo-dark.png",
  "success_url": "/",
  "env": {
    "PORT": {
      "description": "A port number",
      "value": "3800"
    },
    "DEBUG_PREFIX": {
      "description": "Debug prefix",
      "value": "erxes-logs"
    },
    "MONGO_URL": {
      "description": "MONGO URL",
      "value": "MONGO_URL"
    }
  },
  "addons": [
    {
      "plan": "mongolab:sandbox",
      "as": "MONGO"
    }
  ],
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs"
    }
  ]
}
